openapi: 3.0.0
info:
  title: VisionHub
  description: Documentation for the Application API
  version: 1.0.0
servers:
  - url: https://test.com
    description: VisionHub Main Server
paths:
  /auth/register:
    post:
      summary: Register a new user
      description: Create a new user
      deprecated: false
      responses:
        200:
          description: User created successfully
        401:
          description: (Unauthorized) Invalid or missing Access Token
      tags:
        - auth

  /auth/login:
    post:
      summary: Login to the system
      description: Authenticate a user
      deprecated: false
      responses:
        200:
          description: User logged in successfully
        401:
          description: (Unauthorized) Invalid or missing Access Token
      tags:
        - auth

  /auth/checkToken:
    get:
      summary: Validate the token
      description: Check if the token is valid
      deprecated: false
      responses:
        200:
          description: Token is valid
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - auth

  /auth/logout:
    post:
      summary: Logout the user
      description: Delete the user's tokens
      deprecated: false
      responses:
        200:
          description: User logged out successfully
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - auth
        
  /images/public:
    get:
      summary: Display a listing of public images.
      description: Returns a list of all public images.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - images

  /images/user:
    get:
      summary: Get the images uploaded by the user.
      description: Returns a list of images uploaded by the current user.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - images

  /images/all:
    get:
      summary: Get all images.
      description: Returns a list of all images.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - images
  /images/{id}/get:
    get:
      summary: Serve an image.
      description: Returns the specified image.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
        404:
          description: Image not found
      parameters:
        - name: id
          in: path
          required: true
          description: ''
          schema:
            type: string
      security:
        - bearerAuth: []
      tags:
        - image
  /models/upload:
    post:
      summary: Store a newly created 3D model.
      description: Uploads a new 3D model.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - models

  /models/user/{id}:
    get:
      summary: Get the 3D models uploaded by the user.
      description: Returns a list of 3D models uploaded by the specified user.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      parameters:
        - name: id
          in: path
          required: true
          description: ''
          schema:
            type: string
      security:
        - bearerAuth: []
      tags:
        - models

  /models/public:
    get:
      summary: Display a listing of public 3D models.
      description: Returns a list of all public 3D models.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      tags:
        - models

  /model/{id}/thumbnail:
    get:
      summary: Serve a 3D model thumbnail.
      description: Returns the thumbnail for the specified 3D model.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
        403:
          description: Unauthorized
        404:
          description: Model not found
      parameters:
        - name: id
          in: path
          required: true
          description: ''
          schema:
            type: string
      tags:
        - model

  /model/{id}/get:
    get:
      summary: Serve a 3D model.
      description: Returns the specified 3D model.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
        403:
          description: Unauthorized
        404:
          description: Model not found
      parameters:
        - name: id
          in: path
          required: true
          description: ''
          schema:
            type: string
      tags:
        - model
  /user/{id}/get:
    get:
      summary: Get a user.
      description: Returns the specified user.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      parameters:
        - name: id
          in: path
          required: true
          description: ''
          schema:
            type: string
      security:
        - bearerAuth: []
      tags:
        - user

  /user:
    get:
      summary: Get current user.
      description: Returns the user that made the request.
      deprecated: false
      responses:
        200:
          description: OK
        401:
          description: (Unauthorized) Invalid or missing Access Token
      security:
        - bearerAuth: []
      tags:
        - user
tags: []
