        description:
          type: string
          description: string(255)
          nullable: true
          example: string
        steps:
          type: integer
          description: integer
          nullable: false
          example: 1180784195
        cfg:
          type: number
          format: float
          description: float
          nullable: false
        sampler_name:
          type: string
          description: string(255)
          nullable: false
          example: string
        scheduler:
          type: string
          description: string(255)
          nullable: false
          example: string
        created_at:
